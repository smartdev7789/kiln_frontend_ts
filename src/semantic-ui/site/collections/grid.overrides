/*******************************
         Site Overrides
*******************************/

.ui[class*="vertically divided"].grid > .row:first-child:before {
    display: none;
}

.ui[class*="vertically divided"].grid > .row:before {
    width: 100%;
    margin: 0;
    height: 2px;
    background: #C4C4C4;
    box-shadow: none;
}